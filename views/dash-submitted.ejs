<%- include('partials/header')%>
<%- include('partials/nav')%>

<div class="container d-flex flex-column justify-content-center">
  <section class="">
    <div class="m-2 p-1 border-bottom text-center">
      <h1>Submitted Assignments</h1>
    </div>
    <div class="d-flex flex-column justify-content-center">
      <% completedAssignments.forEach((assignment, i)=>{ %>
      <div class="text-center">
        <div class="card">
          <img src="https://images.unsplash.com/photo-1497091071254-cc9b2ba7c48a?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1474&q=80" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title"><%= assignment.parentAssignment.title %></h5>
            <p class="card-text"><%= assignment.parentAssignment.details %></p>
            <a href="/aassignment/<%= assignment.parentAssignment._id %>" class="btn btn-primary">View</a>
          </div>
        </div>
      </div>
      <% }); %>
    </div>
  </section>
</div>
<%- include('partials/footer')%>